<!DOCTYPE html>
<html>
  <head>
    <title>Gaures</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag "https://maps.googleapis.com/maps/api/js?key=AIzaSyAnAjX5ZNh-hfz2NOQSatoPxIphgaC-0UY" %>
    <%= javascript_include_tag "https://hpneo.github.io/gmaps/gmaps.js" %>
    <%= yield :javascript_includes %>
  </head>

  <body>

    <!-- Navbar -->
    <%= render "layouts/navbar" %>

    <!-- Main Container -->
    <div class="container">
      <%= yield %>
    </div>

  </body>
</html>
